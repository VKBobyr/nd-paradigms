doctype html

html
	head
		include includes/head-content

	style.
		.row {
			display: flex;
			align-items: center;
			margin-top: 20px;
		}

		img {
			margin: 10px;
			cursor: pointer;
		}

	body
		include includes/header

		.row
			for lang in languages
				img(src=lang.pictureUrl, onclick=`location.href='${lang.name}'`)
